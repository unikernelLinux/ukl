[submodule "linux"]
	path = linux
	url = https://github.com/unikernelLinux/linux
	update = checkout
[submodule "glibc"]
	path = glibc
	url = git@github.com:unikernelLinux/glibc.git 
	update = checkout
[submodule "gapbs"]
	path = gapbs
	url = https://github.com/unikernelLinux/gapbs
[submodule "dnsmasq/dnsmasq"]
	path = dnsmasq/dnsmasq
	url = https://github.com/rwmjones/dnsmasq
[sudmodule "glibc"]
	branch = ukl-dynamic
[sudmodule "linux"]
	branch = ukl-dynamic
